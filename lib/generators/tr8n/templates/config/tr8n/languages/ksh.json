{
  "locale": "ksh",
  "english_name": "Colognian",
  "context_rules": {
    "date": {
      "keys": ["past", "present", "future"]
    },
    "gender_list": {
      "keys": ["one", "other"],
      "rules": {
        "one": {"rule": ["=", 1, "@size"], "description": "contains 1 element"},
        "other": {"description": "contains at least 2 elements"}
      }
    },
    "gender": {
      "keys": ["male", "female", "unknown"]
    },
    "number": {
      "keys": ["zero", "one", "other"],
      "rules": {
        "zero":   {"rule": {"is":"0"}, "description": "{n} is 0", "examples": "0"},
        "one":    {"rule": {"is":"1"}, "description": "{n} is 1", "examples": "1"},
        "other":  {"examples": "2-999; 0.2, 1.2, 2.07..."}
      }
    }
  },
  "language_cases": {
  }
}