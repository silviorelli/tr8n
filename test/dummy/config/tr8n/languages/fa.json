        "other": {"rule": [">", 1, "@size"], "description": "contains at least 2 elements"}
      }
    },
    "gender": {
      "keys": ["male", "female", "unknown"]
    },
    "number": {
      "keys": ["other"],
      "rules": {
        "other":  {"examples": "0-999; 1.2, 2.07..."}
      }
    }
  },
