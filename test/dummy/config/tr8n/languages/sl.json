{
  "locale": "sl",
  "english_name": "Slovenian",
  "native_name": "Slovenski",
  "google_key": "sl",
  "context_rules": {
    "date": {
      "keys": ["past", "present", "future"]
    },
    "gender_list": {
      "keys": ["one", "other"],
      "rules": {
        "one": {"rule": {"size": {"is":"1"}}, "description": "contains 1 person"},
        "other": {"rule": {"size": {"is_not":"1"}}, "description": "contains at least 2 people"}
      }
    },
    "gender": {
      "keys": ["male", "female", "unknown"]
    },
    "number": {
      "keys": ["one", "two", "few", "other"],
      "rules": {
        "one":    {"rule": {"mod_100_is":"1"}, "description": "{n} mod 100 is 1", "examples": "1, 101, 201, 301, 401, 501..."},
        "two":    {"rule": {"mod_100_is":"2"}, "description": "{n} mod 100 is 2", "examples": "2, 102, 202, 302, 402, 502..."},
        "few":    {"rule": {"mod_100_in":"3..4"}, "description": "{n} mod 100 in 3..4", "examples": "3, 4, 103, 104, 203, 204..."},
        "other":  {"examples": "0, 5-100, 105-200, 205-300...; 1.2, 2.07, 3.94, 5.81..."}
      }
    }
  },
  "language_cases": {
  }
}