{
  "locale": "ti",
  "english_name": "Tigrinya",
  "google_key": "ti",
  "context_rules": {
    "date": {
      "keys": ["past", "present", "future"]
    },
    "gender_list": {
      "keys": ["one", "other"],
      "rules": {
        "one": {"rule": {"size": {"is":"1"}}, "description": "contains 1 person"},
        "other": {"rule": {"size": {"is_not":"1"}}, "description": "contains at least 2 people"}
      }
    },
    "gender": {
      "keys": ["male", "female", "unknown"]
    },
    "number": {
      "keys": ["one", "other"],
      "rules": {
        "one":    {"rule": {"in":"0,1"}, "description": "{n} in 0..1", "examples": "0, 1"},
        "other":  {"examples": "2-999; 1.2, 2.07..."}
      }
    }
  },
  "language_cases": {
  }
}