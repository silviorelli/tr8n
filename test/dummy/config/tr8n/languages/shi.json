{
  "locale": "shi",
  "english_name": "Tachelhit",
  "context_rules": {
    "date": {
      "keys": ["past", "present", "future"]
    },
    "gender_list": {
      "keys": ["one", "other"],
      "rules": {
        "one": {"rule": {"size": {"is":"1"}}, "description": "contains 1 person"},
        "other": {"rule": {"size": {"is_not":"1"}}, "description": "contains at least 2 people"}
      }
    },
    "gender": {
      "keys": ["male", "female", "unknown"]
    },
    "number": {
      "keys": ["one", "few", "other"],
      "rules": {
        "one":    {"rule": {"within":"0..1"}, "description": "{n} within 0..1", "examples": "0, 1"},
        "few":    {"rule": {"in":"2..10"}, "description": "{n} in 2..10", "examples": "2-10"},
        "other":  {"examples": "11-999; 1.2, 2.07..."}
      }
    }
  },
  "language_cases": {
  }
}